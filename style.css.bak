input[type="checkbox"] {
 display: none;

}
 
label {
 width:100%;
 color: #000;
 cursor: default;
 font-weight: normal;
 line-height: 30px;
 padding: 10px 0;
 vertical-align: middle;
 color:#000000;
 
font-family: Verdana;
font-size: 14px;
font-style: normal;
font-weight: 400;
line-height: 17px;
letter-spacing: 0em;
text-align: left;

}
 
label:before {
 content: " ";
 color: #000;
 display: inline-block;
 font: 20px/30px FontAwesome;
 margin-right: 15px;
 position: relative;
 text-align: center;
 text-indent: 0px;
 width: 30px;
 height: 30px;
 background: #FFF;
 border: 1px solid #e3e3e3;
 border-image: initial;
 vertical-align: middle;
}
 
input:checked + label:before {
 content: "\f00c";
}
 
input:disabled + label:before {
 background: #eee;
 color: #aaa;
}

